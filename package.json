{
  "name": "root",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "bootstrap": "pnpm install && pnpm stub",
    "clean": "rimraf node_modules",
    "reinstall": "pnpm clean && pnpm bootstrap",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepare": "is-ci || husky install",
    "lint:lint-staged": "lint-staged -c ./.lintstagedrc",
    "commit": "czg",
    "stub": "turbo run stub"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^4.0.2",
    "dayjs": "^1.11.7",
    "lodash-es": "^4.17.21",
    "path-to-regexp": "^6.2.1",
    "seemly": "^0.3.8",
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@aurora/eslint": "workspace:*",
    "@aurora/prettier": "workspace:*",
    "@aurora/stylelint": "workspace:*",
    "@aurora/ts": "workspace:*",
    "@aurora/vite": "workspace:*",
    "@commitlint/cli": "^17.5.1",
    "@dcloudio/types": "^3.3.2",
    "@types/jsdom": "^21.1.0",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^18.14.2",
    "@vue-macros/volar": "^0.9.5",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "cross-env": "^7.0.3",
    "cz-git": "^1.6.1",
    "czg": "^1.6.1",
    "default-passive-events": "^2.0.0",
    "fast-glob": "^3.2.12",
    "husky": "^8.0.0",
    "is-ci": "^3.0.1",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "postcss-px-to-viewport": "^1.1.1",
    "rimraf": "^4.4.1",
    "sass": "^1.62.0",
    "start-server-and-test": "^2.0.0",
    "turbo": "^1.9.3",
    "typedoc": "^0.25.3",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "~5.0.4",
    "unbuild": "^1.2.0",
    "unplugin-vue-define-options": "^1.3.3",
    "vite": "4.3.0-beta.6",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.8.3"
  }
}
